//give two number p&q.find the value p^q using a recursive function

public class recursion10 {
    static int pow(int p,int q){
        if (q==0){
            return 1;
        }
        int smallans=pow(p,q-1);
        return smallans*p;

    }
    public static void main(String[] args) {
        System.out.println(pow(5, 4));
    }
}

//output
//625
